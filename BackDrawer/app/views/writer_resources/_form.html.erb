  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.0/js/bootstrap-datepicker.min.js"></script>


  <%= form_for @resource do |f| %>
    <% if @resource.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@resource.errors.count, "error") %> prohibited this resource type from being saved:</h2>

        <ul>
        <% @resource.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    
    <div class="form-group field">
      <%= f.label :resource_name, "Name" %><br>
      <%= f.text_field :resource_name, :class => "form-control" %>
    </div>
    <div class="form-group field">
      <%= f.label :details, "Details" %><br>
      <%= f.text_area :details, :class => "form-control" %>
    </div>


    <div class="actions">
      <%= f.submit "Create Resource", :class => "btn" %>
    </div>
  <% end %>
