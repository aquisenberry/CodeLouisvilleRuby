<div class="container-fluid">
  <div class="wrapper">
	<p id="notice"><%= notice %></p>

	<p>
	  <strong>First name:</strong>
	  <%= @user.first_name %>
	</p>

	<p>
	  <strong>Last name:</strong>
	  <%= @user.last_name %>
	</p>

	<p>
	  <strong>DOB:</strong>
	  <%= @user.date_of_birth %>
	</p>

	<p>
	  <strong>Username:</strong>
	  <%= @user.username %>
	</p>

	<p>
	  <strong>Email address:</strong>
	  <%= @user.email_address %>
	</p>


	<%= link_to 'Edit', edit_user_path(@user) %> |
	<%= link_to 'Back', root_path %>
  </div>
</div>
